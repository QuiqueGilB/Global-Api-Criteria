{
  "name": "quiquegilb/global-api-criteria",
  "version": "0.0.1",
  "description": "Global criteria php",
  "keywords": [
    "criteria",
    "api criteria",
    "doctrine",
    "eloquent",
    "array"
  ],
  "homepage": "https://github.com/QuiqueGilB/Global-Api-Criteria",
  "license": "MIT",
  "authors": [
    {
      "name": "Enrique Gil",
      "email": "user@enriquegil.me"
    }
  ],
  "require": {
    "php": ">=7.3"
  },
  "autoload": {
    "psr-4": {
      "QuiqueGilB\\GlobalApiCriteria\\": "src/",
      "QuiqueGilB\\GlobalApiCriteria\\Example\\": "example/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "QuiqueGilB\\GlobalApiCriteria\\Tests\\": "tests/src/",
      "QuiqueGilB\\GlobalApiCriteria\\Example\\Tests\\": "tests/example/"
    }
  },
  "require-dev": {
    "ext-json": "*",
    "ext-pdo": "*",
    "phpunit/phpunit": "^9.5",
    "symfony/var-dumper": "^5.2",
    "illuminate/database": "^8.22",
    "doctrine/orm": "^2.8"
  }
}
